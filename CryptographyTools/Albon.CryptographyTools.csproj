<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Albon.CryptographyTools</Title>
    <Authors>Alexis Bonneau</Authors>
    <Description>Cryptography tools for signing messages</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/alex892-droid/Albon.CryptographyTools</PackageProjectUrl>
    <PackageIcon></PackageIcon>
    <RepositoryUrl>https://github.com/alex892-droid/Albon.CryptographyTools</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>cryptography; signing; private; signature; public; key;</PackageTags>
  </PropertyGroup>

</Project>
